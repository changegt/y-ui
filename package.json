{
  "name": "root",
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "start": "dumi dev",
    "build:doc": "dumi build",
    "pushserver": "node ./scripts/upload.js",
    "build:ui": "cd packages/y-ui && npm run build",
    "deploy:ui": "npm run build:ui && npm run publish",
    "tslint:ui": "cd packages/y-ui && npm run tslint",
    "publish": "lerna publish"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/changegt/y-ui.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "tslint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "devDependencies": {
    "ftp": "^0.3.10",
    "ftp-deploy": "^2.3.7",
    "husky": "^1.3.1",
    "lerna": "2.8.0",
    "lint-staged": "^8.1.5",
    "tslint": "^5.14.0",
    "typescript": "^3.4.5"
  },
  "dependencies": {
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.0.5",
    "antd": "^3.26.13",
    "clipboard": "^2.0.6",
    "crypto-js": "^3.1.9-1",
    "dumi": "^1.0.13",
    "echarts": "^4.8.0",
    "echarts-liquidfill": "^2.0.5",
    "father-build": "^1.17.2",
    "less-loader": "^5.0.0",
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1",
    "querystring": "^0.2.0",
    "react": "^16.12.0",
    "yorkie": "^2.0.0"
  }
}
